// Large tablets and small desktops
@media screen and (max-width: 992px) and (min-width: 769px) {
	.header .nav-tabs {
		margin-left: 80px !important;
	}

	.main {
		padding: 40px 20px !important;
	}
}

// Tablet and smaller screens
@media screen and (max-width: 768px) {
	.header .nav-tabs {
		margin-left: 60px !important;
		font-size: 12px;
	}

	.header .nav-tabs li a,
	.header .nav-tabs .nav-item .nav-link {
		padding: 12px 15px 8px !important;
		font-size: 12px !important;
	}

	.header .user-menu {
		right: 10px;
		font-size: 11px;
	}

	.header .logo-header {
		width: 100px;
	}

	.header .red-base {
		width: 50px;
	}

	// Tablet content adjustments
	.main {
		padding: 30px 15px !important;
	}

	.card-home {
		margin-bottom: 20px;

		h3 {
			font-size: 17px;
		}

		i.fa-3x {
			font-size: 2.5em !important;
		}
	}
}

// Mobile screens
@media screen and (max-width: 480px) {
	.header .nav-tabs {
		margin-left: 10px !important;
		width: calc(100% - 180px);
	}

	.header .nav-tabs li a,
	.header .nav-tabs .nav-item .nav-link {
		padding: 8px 10px 6px !important;
		font-size: 11px !important;
		margin: 0 5px;
	}

	.header .user-menu {
		right: 5px;
		font-size: 10px;
		top: 15px;
	}

	.header .logo-header {
		width: 80px;
		margin-left: 5px;
	}

	.header .red-base {
		width: 40px;
	}

	// Responsive content cards
	.main {
		padding: 20px 10px !important;
	}

	.sidebar-subpage {
		padding: 20px 15px !important;

		h3 {
			font-size: 15px !important;
			line-height: 20px;
		}

		p {
			font-size: 12px !important;
		}
	}

	.card-home {
		margin-bottom: 15px;
		padding: 15px;

		h3 {
			font-size: 16px;
		}

		i.fa-3x {
			font-size: 2em !important;
		}

		p {
			font-size: 11px !important;
		}
	}

	.col-lg-4,
	.col-md-6,
	.col-sm-6 {
		padding-left: 5px;
		padding-right: 5px;
	}
}

@media screen and (max-width: 1024px) {
	.main {
		margin-left: 0;
		padding: 36px 0;
	}

	.sidebar {
		display: none;
		visibility: hidden;
	}

	.header .nav-tabs {
		margin: 0 auto;
		border: 0;
		float: left !important;
		margin-left: 120px;
	}

	.sidebar-subpage {
		height: auto;
		width: 100%;
		position: relative;
		z-index: 1;
		top: 0;
		left: 0;
		background: #e5e9f2;
		align-items: stretch;
		border-right: 1px solid #e6ecf1;
		overflow-x: hidden;
		padding: 30px 30px;

		h3 {
			font-size: 17px;
			margin-bottom: 20px;
			line-height: 25px;
		}

		.list-group {
			border: 0;
			padding: 0;
			padding-top: 0;

			.list-group-item {
				background-color: transparent;
				border: 0;
				border-bottom: 0 solid #ccc;
				-webkit-transition:
					color 300ms,
					background-color 300ms;
				-moz-transition:
					color 300ms,
					background-color 300ms;
				-o-transition:
					color 300ms,
					background-color 300ms;
				transition:
					color 300ms,
					background-color 300ms;
				margin-bottom: 2px;
				padding: 12px;
				border-width: 1px 1px 1px 1px;
				border-style: solid;
				border-color: #e5e9f2;

				a {
					text-decoration: none;
					font-weight: 700;
					color: @darkblue;
					-webkit-transition:
						color 300ms,
						background-color 300ms;
					-moz-transition:
						color 300ms,
						background-color 300ms;
					-o-transition:
						color 300ms,
						background-color 300ms;
					transition:
						color 300ms,
						background-color 300ms;
					border-width: 1px 1px 1px 1px;
					border-color: transparent;
					border-left: 4px solid transparent;
				}

				&:hover {
					text-decoration: none;
					-webkit-transition:
						color 300ms,
						background-color 300ms;
					-moz-transition:
						color 300ms,
						background-color 300ms;
					-o-transition:
						color 300ms,
						background-color 300ms;
					transition:
						color 300ms,
						background-color 300ms;
					cursor: pointer;
					display: flex;
					box-shadow: rgba(116, 129, 141, 0.1) 0 3px 8px 0;
					align-items: center;
					-webkit-box-align: center;
					border-width: 1px 1px 1px 1px;
					border-style: solid;
					border-color: rgb(211, 220, 228) rgb(211, 220, 228) rgb(211, 220, 228) rgb(247, 125, 5);
					border-image: initial;
					background: rgb(255, 255, 255);
					border-left: 4px solid @red_snello;
					border-radius: 3px;
					color: @grey_snello;
				}

				&:active {
					background-color: @white;
					text-decoration: none;
				}
			}
		}
	}

	.sidenav {
		display: none;
		visibility: hidden;
	}

	.content {
		padding: 10px 0 !important;
	}

	.card {
		padding: 20px;
		border: 1px solid #ddd;
		text-decoration: none !important;
		height: 150px;
		background-color: #ffffff;
		margin-bottom: 30px;
		box-shadow: 0 2px 8px rgba(0, 0, 0, 0.1);
		border-radius: 4px;
		transition:
			color 300ms,
			box-shadow 300ms;
		p {
			font-size: 11px;
			margin-top: 5px;
		}
	}
}

