{
	"useTabs": true,
	"tabWidth": 4,
	"singleQuote": true,
	"printWidth": 120,
	"semi": true,
	"trailingComma": "none",
	"bracketSameLine": true,
	"overrides": [
		{
			"files": "*.html",
			"options": {
				"parser": "angular",
				"htmlWhitespaceSensitivity": "strict",
				"bracketSameLine": true
			}
		}
	]
}
